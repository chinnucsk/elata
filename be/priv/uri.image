--slope-mode
--full-size-mode
--lower-limit 0
--vertical-label sec
--rigid
--border 0
--width 720
--height 240
--title "uri <?title?>"
--start <?from?>
--end now
DEF:_e=<?data?>/<?host?>/<?uid?>/uri:val:AVERAGE

LINE1:u#99CC33
LINE1:e#333300:latency
LINE1:b#99CC33


VDEF:emin=e,MINIMUM
VDEF:eavg=e,AVERAGE
VDEF:estd=e,STDEV
VDEF:ep95=e,95,PERCENTNAN
VDEF:ep98=e,98,PERCENTNAN
VDEF:emax=e,MAXIMUM
GPRINT:emin:"min %1.3lf"
GPRINT:eavg:"avg %1.3lf"
GPRINT:estd:"std %1.3lf"
